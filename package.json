{
  "name": "lsp-log-parser",
  "version": "1.0.0",
  "description": "Parser for LSP server logs",
  "author": "Rafał Chłodnicki",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore . && npm run csslint",
    "csslint": "stylelint '**/*.scss' '**/*.vue'",
    "typecheck": "npx tsc"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "yarn lint",
    "*.{scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.0",
    "nuxt": "^2.15.3",
    "vue-json-pretty": "1.8.3",
    "vue-shortkey": "^3.1.7"
  },
  "devDependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.4",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "stylelint": "^14.5.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^27.0.0",
    "vscode-languageserver-protocol": "^3.16.0",
    "vue": "2.6",
    "vue-server-renderer": "2.6",
    "vue-template-compiler": "2.6"
  }
}
