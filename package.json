{
  "name": "lsp-log-parser",
  "version": "1.0.0",
  "description": "Parser for LSP server logs",
  "author": "Rafał Chłodnicki",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "yarn jslint && yarn csslint",
    "jslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "csslint": "stylelint '**/*.scss' '**/*.vue'",
    "typecheck": "npx tsc"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --ext .js,.ts,.vue --ignore-path .gitignore",
    "*.{scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typecheck"
    }
  },
  "dependencies": {
    "nuxt": "^3.3.2",
    "vue-json-pretty": "1.9.4",
    "vue-shortkey": "^3.1.7"
  },
  "devDependencies": {
    "@nuxt/types": "^2.16.3",
    "@nuxt/typescript-build": "^3.0.0",
    "@nuxtjs/stylelint-module": "^4.2.1",
    "@nuxtjs/vuetify": "^1.12.3",
    "@types/node": "^16.18.21",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^31.0.0",
    "typescript": "^4.9.5",
    "vscode-languageserver-protocol": "^3.17.3",
    "vuex": "3"
  },
  "resolutions": {
    "@nuxt/typescript-build/typescript": "^4.9.5"
  }
}
