{
  "name": "lsp-log-parser",
  "version": "1.0.0",
  "description": "Parser for LSP server logs",
  "author": "Rafał Chłodnicki",
  "private": true,
  "scripts": {
    "prepare": "nuxi prepare",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi preview",
    "generate": "nuxi generate",
    "lint": "yarn jslint && yarn csslint",
    "jslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "csslint": "stylelint '**/*.scss' '**/*.vue'",
    "typecheck": "npx tsc"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --ext .js,.ts,.vue --ignore-path .gitignore",
    "*.{scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn typecheck"
    }
  },
  "dependencies": {
    "@vueuse/core": "^10.1.0",
    "@vueuse/nuxt": "^10.1.0",
    "nuxt": "^3.3.3",
    "vue-json-pretty": "^2.2.4",
    "vuetify": "^3.1.13"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@nuxtjs/google-fonts": "^3.0.0",
    "@nuxtjs/stylelint-module": "^5.0.1",
    "@types/node": "^16.18.23",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^9.10.0",
    "eslint-plugin-vuetify": "^2.0.0-beta.3",
    "husky": "^4.3.8",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "sass": "^1.61.0",
    "stylelint": "^15.4.0",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^32.0.0",
    "typescript": "^5.0.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vscode-languageserver-protocol": "^3.17.3"
  }
}
