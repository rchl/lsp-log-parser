{
  "name": "lsp-log-parser",
  "version": "1.0.0",
  "description": "Parser for LSP server logs",
  "author": "Rafał Chłodnicki",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi preview",
    "generate": "nuxi generate",
    "prepare": "nuxi prepare",
    "lint": "yarn csslint && yarn csslint",
    "jslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "csslint": "stylelint '**/*.scss' '**/*.vue'",
    "typecheck": "npx tsc",
    "postinstall": "patch-package"
  },
  "disable_lint-staged": {
    "*.{js,ts,vue}": "yarn jslint",
    "*.{scss,vue}": "yarn csslint"
  },
  "disable_husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "dependencies": {
    "nuxt-edge": "latest",
    "patch-package": "^6.4.7",
    "vue-json-pretty": "1.8.3",
    "vue-shortkey": "^3.1.7"
  },
  "devDependencies": {
    "@nuxt/bridge": "npm:@nuxt/bridge-edge",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^4.3.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-html": "^1.5.0",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.8.3",
    "vscode-languageserver-protocol": "^3.17.2",
    "vuex": "3"
  }
}
